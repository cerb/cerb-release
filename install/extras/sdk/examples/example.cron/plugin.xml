<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.devblocks.com/schema/plugin.xsd">
	<id>example.cron</id>
	<name>[Examples] Example Cron</name>
	<description>This example plugin adds an example cron</description>
	<author>Webgroup Media, LLC.</author>
	<version>0.0.0</version>
	<link>https://cerb.ai</link>
	
	<!-- Plugin Dependencies -->
	
	<dependencies>
		<require plugin_id="cerberusweb.core" version="9.4" />
	</dependencies>
	
	<!-- Exported Classes -->
	
	<extensions>
		
		<!-- Scheduled Jobs -->
		
		<extension point="cerberusweb.cron">
			<id>example.cron</id>
			<name>Example Cron</name>
			<class>
				<file>api/App.php</file>
				<name>ExCron</name>
			</class>
			<params />
		</extension>
		
	</extensions>
</plugin>