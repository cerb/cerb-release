<h2>Icon Reference</h2>

<div style="column-width:200px;margin-bottom:20px;">
    {foreach from=$icons item=icon}
        <div>
            <span class="glyphicons glyphicons-{$icon}" title=".glyphicons-{$icon}" style="font-size:200%;margin:5px;"></span>
            <span style="vertical-align:bottom;">{$icon}</span>
        </div>
    {/foreach}
</div>

