{
  "package": {
    "name": "Chart: Time Series",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Chart: Time Series",
      "uri": "cerb_workspace_widget_chart_time_series",
      "description": "A data query driven time series visualization",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "picklist",
          "label": "Dataset:",
          "key": "prompt_dataset",
          "params": {
            "options": {
              "Tickets created per month by group": "tickets_per_month_by_group",
              "Tickets created per week by group": "tickets_per_week_by_group"
            }
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "widget_tickets_per_month_by_group",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'tickets_per_month_by_group' %}}true{{% endif %}}",
      "label": "Tickets Created Per Month By Group",
      "extension_id": "cerb.workspace.widget.chart.timeseries",
      "tab_id": "{{{workspace_tab_id}}}",
      "pos": 1,
      "width_units": 4,
      "zone": "content",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:ticket\r\nby.count:[created@month,group~10]\r\nquery:(\r\n  created:\"first day of this month -1 year to now\"\r\n)\r\nformat:timeseries",
        "cache_secs": "",
        "chart_as": "bar_stacked",
        "xaxis_label": "",
        "yaxis_label": "",
        "yaxis_format": "number",
        "height": "",
        "options": {
          "show_legend": "1"
        }
      }
    },
    {
      "uid": "widget_tickets_per_week_by_group",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'tickets_per_week_by_group' %}}true{{% endif %}}",
      "label": "Tickets Created Per Week By Group",
      "extension_id": "cerb.workspace.widget.chart.timeseries",
      "tab_id": "{{{workspace_tab_id}}}",
      "pos": 1,
      "width_units": 4,
      "zone": "content",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:ticket\r\nby.count:[created@week,group~10]\r\nquery:(\r\n  created:\"first day of this month -1 month to now\"\r\n)\r\nformat:timeseries",
        "cache_secs": "",
        "chart_as": "bar_stacked",
        "xaxis_label": "",
        "yaxis_label": "",
        "yaxis_format": "number",
        "height": "",
        "options": {
          "show_legend": "1"
        }
      }
    }
  ]
}