{
  "package": {
    "name": "Chart: Categories",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Chart: Categories",
      "uri": "cerb_workspace_widget_chart_categories",
      "description": "A data query driven categories visualization",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "picklist",
          "label": "Dataset:",
          "key": "prompt_dataset",
          "params": {
            "options": {
              "Messages sent per worker by group": "messages_per_worker_by_group",
              "Tickets per group by status": "tickets_per_group_by_status"
            }
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "widget_messages_per_worker_by_group",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'messages_per_worker_by_group' %}}true{{% endif %}}",
      "label": "Messages Sent Per Worker By Group",
      "extension_id": "cerb.workspace.widget.chart.categories",
      "tab_id": "{{{workspace_tab_id}}}",
      "pos": 1,
      "width_units": 2,
      "zone": "left",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:message\r\nby:[worker~10,group~10]\r\nquery:(\r\n\tcreated:\"big bang to now\"\r\n\tworker:(isDisabled:n id:>0)\r\n\tisOutgoing:y\r\n\tisBroadcast:n\r\n)\r\nformat:categories",
        "cache_secs": "",
        "xaxis_format": "text",
        "yaxis_format": "number",
        "height": ""
      }
    },
    {
      "uid": "widget_tickets_per_group_by_status",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'tickets_per_group_by_status' %}}true{{% endif %}}",
      "label": "Tickets Per Group By Status",
      "extension_id": "cerb.workspace.widget.chart.categories",
      "tab_id": "{{{workspace_tab_id}}}",
      "pos": 1,
      "width_units": 2,
      "zone": "left",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:ticket\r\nby:[group~10,status]\r\nquery:()\r\nformat:categories",
        "cache_secs": "",
        "xaxis_format": "text",
        "yaxis_format": "number",
        "height": ""
      }
    }
  ]
}