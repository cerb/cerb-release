{
  "package": {
    "name": "Table",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Table",
      "uri": "cerb_workspace_widget_chart_table",
      "description": "Tabular results from a data query",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "picklist",
          "label": "Dataset:",
          "key": "prompt_dataset",
          "params": {
            "options": {
              "Bot Behaviors by Event": "behaviors_by_event",
              "Tickets by Status": "tickets_by_status",
              "Worker Reply Leaderboard": "worker_reply_leaderboard",
              "Workers By Group": "workers_by_group"
            }
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "workspace_widget_behaviors_by_event",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'behaviors_by_event' %}}true{{% endif %}}",
      "label": "Bot Behaviors by Event",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "cerb.workspace.widget.chart.table",
      "pos": 1,
      "width_units": 2,
      "zone": "content",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:behavior\r\nby.count:[event~100]\r\nquery:()\r\nformat:table",
        "cache_secs": ""
      }
    },
    {
      "uid": "workspace_widget_workers_by_group",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'workers_by_group' %}}true{{% endif %}}",
      "label": "Workers By Group",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "cerb.workspace.widget.chart.table",
      "pos": 1,
      "width_units": 2,
      "zone": "content",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:worker\r\nby.count:[group~100]\r\nquery:()\r\nformat:table",
        "cache_secs": ""
      }
    },
    {
      "uid": "workspace_widget_worker_leaderboard",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'worker_reply_leaderboard' %}}true{{% endif %}}",
      "label": "Reply Leaderboard (Top 10, All-Time)",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "cerb.workspace.widget.chart.table",
      "pos": 1,
      "width_units": 2,
      "zone": "content",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:message\r\nby:[worker~10]\r\nquery:(\r\n\tcreated:\"big bang to now\"\r\n\tworker.id:!0\r\n\tisOutgoing:y\r\n\tisBroadcast:n\r\n)\r\nformat:table",
        "cache_secs": ""
      }
    },
    {
      "uid": "workspace_widget_tickets_by_status",
      "_context": "cerberusweb.contexts.workspace.widget",
      "_exclude": "{{% if prompt_dataset != 'tickets_by_status' %}}true{{% endif %}}",
      "label": "Tickets by Status",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "cerb.workspace.widget.chart.table",
      "pos": 1,
      "width_units": 2,
      "zone": "content",
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:ticket\r\nby:[status]\r\nquery:()\r\nformat:table",
        "cache_secs": ""
      }
    }
  ]
}