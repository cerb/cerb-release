{
  "package": {
    "name": "Donut",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Donut",
      "uri": "cerb_workspace_widget_donut",
      "description": "A data query driven donut chart visualization",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "picklist",
          "label": "Dataset:",
          "key": "prompt_dataset",
          "params": {
            "options": {
              "Orgs by Country": "orgs_by_country",
              "Tickets by Status": "tickets_by_status"
            }
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "widget_donut_tickets_by_status",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'tickets_by_status' %}}true{{% endif %}}",
      "label": "Tickets by Status",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "cerb.workspace.widget.chart.pie",
      "pos": 0,
      "width_units": 2,
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:ticket\r\nby:[status]\r\nquery:()\r\nformat:pie",
        "cache_secs": "",
        "chart_as": "donut",
        "height": "",
        "options": {
          "show_legend": "1"
        }
      }
    },
    {
      "uid": "widget_donut_orgs_by_country",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'orgs_by_country' %}}true{{% endif %}}",
      "label": "Orgs by Country",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "cerb.workspace.widget.chart.pie",
      "pos": 0,
      "width_units": 2,
      "params": {
        "data_query": "type:worklist.subtotals\r\nof:org\r\nby:[country~20]\r\nquery:(country:!\"\")\r\nformat:pie",
        "cache_secs": "",
        "chart_as": "donut",
        "height": "",
        "options": {
          "show_legend": "1"
        }
      }
    }
  ]
}