{
  "package": {
    "name": "Map: World Countries",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Map: World Countries",
      "uri": "cerb_workspace_widget_map_world",
      "description": "A zoomable map of world countries with points of interest from a data query",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "map_world",
      "_context": "cerberusweb.contexts.workspace.widget",
      "label": "Map: World Countries",
      "extension_id": "cerb.workspace.widget.map.geopoints",
      "tab_id": "{{{workspace_tab_id}}}",
      "pos": 0,
      "width_units": 2,
      "params": {
        "projection": "world",
        "data_query": "{# See: https://cerb.ai/docs/data-queries/worklist/geopoints/ #}\r\ntype:sample.geo.points\r\nseries.cities:(\r\n  lihue:(name:\"Lihue\" coordinates:\"POINT(-159.342222 21.978611)\")\r\n  new_york:(name:\"New York\" coordinates:\"POINT(-73.935242 40.73061)\")\r\n  seattle:(name:\"Seattle\" coordinates:\"POINT(-122.350876 47.620499)\")\r\n  berlin:(name:\"Berlin\" coordinates:\"POINT(13.393236 52.504043)\")\r\n  sydney:(name: \"Sydney\" coordinates:\"POINT(151.21 -33.868)\")\r\n)\r\nformat:geojson"
      }
    }
  ]
}