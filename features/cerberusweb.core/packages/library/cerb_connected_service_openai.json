{
  "package": {
    "name": "Connected Service: OpenAI",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "OpenAI",
      "uri": "cerb_connected_service_openai",
      "description": "Integration with OpenAI",
      "instructions": "# Instructions\r\n\r\n* Enter your API key from <https://platform.openai.com/account/api-keys>",
      "point": "connected_service",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [],
      "prompts": [
        {
          "type": "text",
          "label": "API Key",
          "key": "prompt_api_key",
          "params": {
            "default": "",
            "placeholder": "(paste your API Key)"
          }
        },
        {
          "type": "text",
          "label": "Alias",
          "key": "prompt_name",
          "params": {
            "default": "openai",
            "placeholder": "(enter a URI, a-z,0-9,_)"
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "service_openai",
      "_context": "connected_service",
      "name": "OpenAI",
      "extension_id": "cerb.service.provider.token.bearer",
      "params": {
        "token_name": "Bearer"
      }
    },
    {
      "uid": "account_openai",
      "_context": "connected_account",
      "name": "OpenAI",
      "service_id": "{{{uid.service_openai}}}",
      "owner__context": "cerberusweb.contexts.app",
      "owner_id": "0",
      "params": {
        "token": "{{{prompt_api_key}}}"
      }
    }
  ]
}