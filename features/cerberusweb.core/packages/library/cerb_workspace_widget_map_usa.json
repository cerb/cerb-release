{
  "package": {
    "name": "Map: U.S. States",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Map: U.S. States",
      "uri": "cerb_workspace_widget_map_usa",
      "description": "A zoomable map of U.S. states with points of interest from a data query",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "map_usa",
      "_context": "cerberusweb.contexts.workspace.widget",
      "label": "Map: U.S. States",
      "extension_id": "cerb.workspace.widget.map.geopoints",
      "tab_id": "{{{workspace_tab_id}}}",
      "pos": 1,
      "width_units": 2,
      "zone": "left",
      "params": {
        "projection": "usa",
        "data_query": "{# See: https://cerb.ai/docs/data-queries/worklist/geopoints/ #}\r\ntype:sample.geo.points\r\nseries.cities:(\r\n  new_york:(name:\"New York\" coordinates:\"POINT(-73.935242 40.73061)\")\r\n  seattle:(name:\"Seattle\" coordinates:\"POINT(-122.350876 47.620499)\")\r\n  san_francisco:(name:\"San Francisco\" coordinates:\"POINT(-122.446747 37.733795)\")\r\n  los_angeles:(name:\"Los Angeles\" coordinates:\"POINT(-118.2436800 34.0522300)\")\r\n  miami:(name:\"Miami\" coordinates:\"POINT(-80.1936600 25.7742700)\")\r\n  denver:(name:\"Denver\" coordinates:\"POINT(-104.9847000 39.7391500)\")\r\n  houston:(name:\"Houston\" coordinates:\"POINT(-95.3632700 29.7632800)\")\r\n)\r\nformat:geojson"
      }
    }
  ]
}