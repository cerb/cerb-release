{
  "package": {
    "library": {
      "name": "Dashboard with filters",
      "uri": "cerb_workspace_tab_dashboard_with_filters",
      "description": "A dashboard with customizable widgets that update from user-editable filters",
      "point": "workspace_tab",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Page",
          "key": "workspace_page_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.page",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "text",
          "label": "Name:",
          "key": "prompt_dashboard_name",
          "params": {
            "placeholder": "(e.g. Dashboard)",
            "default": "Dashboard"
          }
        },
        {
          "type": "picklist",
          "label": "Layout:",
          "key": "prompt_layout",
          "params": {
            "options": {
              "Open": "",
              "Left Sidebar": "sidebar_left",
              "Right Sidebar": "sidebar_right",
              "3-Column": "thirds"
            },
            "default": ""
          }
        }
      ],
      "placeholders": [

      ]
    }
  },
  "records": [
    {
      "uid": "workspace_tab_dashboard",
      "_context": "workspace_tab",
      "name": "{{{prompt_dashboard_name}}}",
      "page_id": "{{{workspace_page_id}}}",
      "extension_id": "core.workspace.tab.dashboard",
      "params": {
        "layout": "{{{prompt_layout}}}",
        "placeholder_prompts": "---\r\nplaceholder: input_date_range\r\nlabel: \"Date range:\"\r\ntype: date_range\r\ndefault: today -1 week\r\n---\r\nplaceholder: input_date_subtotal_by\r\nlabel: \"By:\"\r\ntype: picklist\r\ndefault: day\r\nparams:\r\n  options: [day, week, month, year]\r\n---\r\nplaceholder: input_statuses\r\nlabel: \"Statuses:\"\r\ntype: picklist\r\ndefault: []\r\nparams:\r\n  multiple: true\r\n  options: [open, waiting, closed, deleted]\r\n---\r\nplaceholder: input_groups\r\nlabel: \"Groups:\"\r\ntype: chooser\r\ndefault:\r\nparams:\r\n  context: group\r\n  single: false"
      }
    },
    {
      "uid": "widget_example",
      "_context": "workspace_widget",
      "label": "Filtered Chart",
      "extension_id": "cerb.workspace.widget.chart.timeseries",
      "tab_id": "{{{uid.workspace_tab_dashboard}}}",
      "pos": 1,
      "width_units": 4,
      "zone": "content",
      "params": {
        "data_query": "{# These placeholders are automatically set by the dashboard filters #}\r\n{# See: https://cerb.ai/guides/dashboards/filters/ #}\r\ntype:worklist.subtotals\r\nof:ticket\r\nby.count:[created@{{input_date_subtotal_by}},group~10]\r\nquery:(\r\n  created:\"{{input_date_range}}\"\r\n  {% if input_groups %}\r\n  group.id:[{{input_groups}}]\r\n  {% endif %}\r\n  {% if input_statuses %}\r\n  status:[{{input_statuses|join(',')}}]\r\n  {% endif %}\r\n)\r\nformat:timeseries",
        "cache_secs": "",
        "chart_as": "bar_stacked",
        "xaxis_label": "",
        "yaxis_label": "",
        "yaxis_format": "number",
        "height": "",
        "options": {
          "show_legend": "1"
        }
      }
    }
  ]
}