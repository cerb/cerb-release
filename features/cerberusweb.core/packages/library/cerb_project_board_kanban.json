{
  "package": {
    "name": "Project Board: Kanban Template",
    "revision": 2019012601,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Kanban",
      "uri": "cerb_project_board_kanban",
      "description": "A three column task management board",
      "point": "project_board",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "text",
          "label": "Project Name:",
          "key": "prompt_project_name",
          "params": {
            "default": "Kanban Project Board",
            "placeholder": "(the name of your project board)"
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "card_task_instructions",
      "_context": "task",
      "title": "Example Task Card",
      "importance": "50",
      "status": "open"
    },
    {
      "uid": "project_board_kanban",
      "_context": "cerberusweb.contexts.project.board",
      "name": "{{{prompt_project_name}}}",
      "owner__context": "app",
      "owner_id": "0",
      "columns": [
        "{{{uid.project_board_kanban_todo}}}",
        "{{{uid.project_board_kanban_in_progress}}}",
        "{{{uid.project_board_kanban_completed}}}"
      ],
      "params": {
        "card_queries": {
          "cerberusweb.contexts.task": "status:[o,w]"
        },
        "card_templates": {
          "cerberusweb.contexts.task": "{% spaceless %}\n{% if is_completed %}\n<div style=\"font-size:120%;\">\n\t<span class=\"glyphicons glyphicons-circle-ok\" style=\"color:rgb(0,150,0);\"></span> Completed!\n</div>\n{% endif %}\n{% endspaceless %}"
        },
        "contexts": [
          "cerberusweb.contexts.task"
        ]
      }
    },
    {
      "uid": "project_board_kanban_todo",
      "_context": "cerberusweb.contexts.project.board.column",
      "name": "TODO",
      "board_id": "{{{uid.project_board_kanban}}}",
      "params": {
      },
      "cards": [
        "task:{{{uid.card_task_instructions}}}"
      ]
    },
    {
      "uid": "project_board_kanban_in_progress",
      "_context": "cerberusweb.contexts.project.board.column",
      "name": "In Progress",
      "board_id": "{{{uid.project_board_kanban}}}",
      "params": {
      },
      "cards": [

      ]
    },
    {
      "uid": "project_board_kanban_completed",
      "_context": "cerberusweb.contexts.project.board.column",
      "name": "Completed!",
      "board_id": "{{{uid.project_board_kanban}}}",
      "params": {
        "actions": {
          "task_status": {
            "status_id": "1"
          }
        }
      },
      "cards": [

      ]
    }
  ]
}