{
  "package": {
    "name": "Clock",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Clock",
      "uri": "cerb_workspace_widget_clock",
      "description": "A clock widget with a configurable timezone",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "picklist",
          "label": "Timezone:",
          "key": "prompt_timezone",
          "params": {
            "default": "los_angeles",
            "options": {
              "Berlin": "berlin",
              "Denver": "denver",
              "Johannesburg": "johannesburg",
              "Kolkata": "kolkata",
              "London": "london",
              "Los Angeles": "los_angeles",
              "Moscow": "moscow",
              "New York": "new_york",
              "Sao Paulo": "sao_paulo",
              "Shanghai": "shanghai",
              "Sydney": "sydney",
              "Tokyo": "tokyo",
              "Winnipeg": "winnipeg"
            }
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "widget_clock_berlin",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_timezone != 'berlin' %}}true{{% endif %}}",
      "label": "Berlin",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "core.workspace.widget.clock",
      "pos": 0,
      "width_units": 1,
      "params": {
        "timezone": "Europe/Berlin",
        "format": "0",
        "color": "#007cbd"
      }
    },
    {
      "uid": "widget_clock_denver",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_timezone != 'denver' %}}true{{% endif %}}",
      "label": "Denver",
      "tab_id": "{{{workspace_tab_id}}}",
