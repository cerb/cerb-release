{
  "package": {
    "name": "Worklist",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Worklist",
      "uri": "cerb_workspace_widget_worklist",
      "description": "A filterable, pageable, and sortable worklist of records",
      "point": "workspace_widget",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Workspace Dashboard",
          "key": "workspace_tab_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.workspace.tab",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "picklist",
          "label": "Dataset:",
          "key": "prompt_dataset",
          "params": {
            "options": {
              "Active Workers": "workers_active",
              "My Reminders": "reminders_my",
              "My Tasks": "tasks_my",
              "My Tickets": "tickets_my",
              "Open Tickets": "tickets_open",
              "Overdue Tasks": "tasks_overdue"
            }
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "widget_worklist_my_reminders",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'reminders_my' %}}true{{% endif %}}",
      "label": "My Reminders",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "core.workspace.widget.worklist",
      "pos": 0,
      "width_units": 2,
      "params": {
        "context": "cerberusweb.contexts.reminder",
        "query_required": "worker:me closed:n",
        "query": "",
        "render_limit": "10",
        "header_color": "#6a87db",
        "columns": [
          "r_name",
          "r_remind_at",
          "r_updated_at"
        ]
      }
    },
    {
      "uid": "widget_worklist_my_tasks",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'tasks_my' %}}true{{% endif %}}",
      "label": "My Tasks",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "core.workspace.widget.worklist",
      "pos": 0,
      "width_units": 2,
      "params": {
        "context": "cerberusweb.contexts.task",
        "query_required": "owner:me status:o",
        "query": "",
        "render_limit": "10",
        "header_color": "#6a87db",
        "columns": [
          "t_due_date",
          "t_importance",
          "t_updated_date"
        ]
      }
    },
    {
      "uid": "widget_worklist_overdue_tasks",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'tasks_overdue' %}}true{{% endif %}}",
      "label": "Overdue Tasks",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "core.workspace.widget.worklist",
      "pos": 0,
      "width_units": 2,
      "params": {
        "context": "cerberusweb.contexts.task",
        "query_required": "status:o due:\"-10 years to now\"",
        "query": "",
        "render_limit": "10",
        "header_color": "#6a87db",
        "columns": [
          "t_due_date",
          "t_importance",
          "t_updated_date"
        ]
      }
    },
    {
      "uid": "widget_my_tickets",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'tickets_my' %}}true{{% endif %}}",
      "label": "My Tickets",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "core.workspace.widget.worklist",
      "pos": 0,
      "width_units": 2,
      "params": {
        "context": "cerberusweb.contexts.ticket",
        "query_required": "owner:me status:o",
        "query": "",
        "render_limit": "10",
        "header_color": "#6a87db",
        "columns": [
          "t_last_wrote_address_id",
          "t_updated_date",
          "t_group_id",
          "t_bucket_id"
        ]
      }
    },
    {
      "uid": "widget_open_tickets",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'tickets_open' %}}true{{% endif %}}",
      "label": "Open Tickets",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "core.workspace.widget.worklist",
      "pos": 0,
      "width_units": 4,
      "params": {
        "context": "cerberusweb.contexts.ticket",
        "query_required": "status:o",
        "query": "",
        "render_limit": "10",
        "header_color": "#6a87db",
        "columns": [
          "t_last_wrote_address_id",
          "t_group_id",
          "t_bucket_id",
          "t_owner_id",
          "t_updated_date"
        ]
      }
    },
    {
      "uid": "widget_currently_active",
      "_context": "workspace_widget",
      "_exclude": "{{% if prompt_dataset != 'workers_active' %}}true{{% endif %}}",
      "label": "Currently Active Workers",
      "tab_id": "{{{workspace_tab_id}}}",
      "extension_id": "core.workspace.widget.worklist",
      "pos": 0,
      "width_units": 2,
      "params": {
        "context": "cerberusweb.contexts.worker",
        "query_required": "lastActivity:\"-30 mins\"",
        "query": "",
        "render_limit": "5",
        "header_color": "#6a87db",
        "columns": [
          "w_title",
          "a_address_email",
          "w_is_superuser",
          "w_at_mention_name",
          "w_language",
          "w_timezone"
        ]
      }
    }
  ]
}