{
  "package": {
    "name": "Bot Behavior Action: Prompt For Input",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Prompt for input",
      "uri": "cerb_bot_behavior_action_interaction_prompt",
      "description": "Prompt for user input",
      "instructions": "# Input Type:\r\n\r\n* **Buttons:** Select from a list of pre-defined responses\r\n* **Chooser:** Select one or more records with a helper popup or autocomplete text box\r\n* **Date:** Enter an absolute or relative date, or select a date from a calendar\r\n* **File Upload:** Upload a file\r\n* **Images:** Select from a list of pre-defined responses represented by images\r\n* **Text:** Enter single line of free-form text\r\n* **Wait:** Pause before a long-running action\r\n\r\nSee: [Prompting for user input](https://cerb.ai/guides/bots/prompts/)",
      "point": "behavior_action:event.message.chat.worker",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [

      ],
      "prompts": [
        {
          "type": "chooser",
          "label": "Behavior",
          "key": "behavior_id",
          "hidden": true,
          "params": {
            "context": "cerberusweb.contexts.behavior",
            "single": true,
            "query": ""
          }
        },
        {
          "type": "text",
          "label": "Parent ID",
          "key": "parent_id",
          "hidden": true,
          "params": {
          }
        },
        {
          "type": "picklist",
          "label": "Input type:",
          "key": "prompt_input_type",
          "params": {
            "default": "text",
            "options": {
              "Buttons": "buttons",
              "Chooser": "chooser",
              "Date": "date",
              "File Upload": "file",
              "Images": "images",
              "Text": "text",
              "Wait": "wait"
            }
          }
        }
      ]
    }
  },
  "behavior_nodes": [
    {
      "uid": "node_prompt_buttons",
      "behavior_id": "{{{behavior_id}}}",
      "parent_id": "{{{parent_id}}}",
      "_exclude": "{{% if prompt_input_type != 'buttons' %}}true{{% endif %}}",
      "type": "action",
      "title": "Prompt with buttons",
      "status": "live",
      "params": {
        "actions": [
          {
            "action": "send_message",
            "message": "Who is your favorite digital assistant?",
            "format": "",
            "delay_ms": "1000"
          },
          {
            "action": "prompt_buttons",
            "options": "Alexa\r\nSiri\r\nCerb",
            "color_from": "#4795f7",
            "color_mid": "#4795f7",
            "color_to": "#4795f7",
            "style": "",
            "var": "prompt_answer",
            "var_format": "{{message}}",
            "var_validate": ""
          }
        ]
      }
    },
    {
      "uid": "node_prompt_text",
      "behavior_id": "{{{behavior_id}}}",
      "parent_id": "{{{parent_id}}}",
      "_exclude": "{{% if prompt_input_type != 'text' %}}true{{% endif %}}",
      "type": "action",
      "title": "Prompt for text",
      "status": "live",
      "params": {
        "actions": [
          {
            "action": "send_message",
            "message": "What is your name?",
            "format": "",
            "delay_ms": "1000"
          },
          {
            "action": "prompt_text",
            "placeholder": "",
            "default": "",
            "mode": "",
            "var": "prompt_answer",
            "var_format": "{{message}}",
            "var_validate": "{% if prompt_answer is empty %}\r\nYou must type something!\r\n{% endif %}"
          }
        ]
      }
    },
    {
      "uid": "node_prompt_date",
      "behavior_id": "{{{behavior_id}}}",
      "parent_id": "{{{parent_id}}}",
      "_exclude": "{{% if prompt_input_type != 'date' %}}true{{% endif %}}",
      "type": "action",
      "title": "Prompt for date",
      "status": "live",
      "params": {
        "actions": [
          {
            "action": "send_message",
            "message": "When?",
            "format": "",
            "delay_ms": "1000"
          },
          {
            "action": "prompt_date",
            "placeholder": "(enter any future date; e.g. \"tomorrow 8am\")",
            "default": "",
            "mode": "",
            "var": "prompt_answer",
            "var_format": "{{message|date('U')}}",
            "var_validate": "{% if 0 == prompt_answer %}\r\nPlease enter a valid date. For instance, \"Friday noon\".\r\n{% elseif prompt_answer < 'now'|date('U') %}\r\nPlease enter a date in the future\r\n{% endif %}"
          }
        ]
      }
    },
    {
      "uid": "node_prompt_chooser",
      "behavior_id": "{{{behavior_id}}}",
      "parent_id": "{{{parent_id}}}",
      "_exclude": "{{% if prompt_input_type != 'chooser' %}}true{{% endif %}}",
      "type": "action",
      "title": "Prompt with chooser",
      "status": "live",
      "params": {
        "actions": [
          {
            "action": "send_message",
            "message": "Pick someone:",
            "format": "",
            "delay_ms": "1000"
          },
          {
            "action": "prompt_chooser",
            "context": "cerberusweb.contexts.worker",
            "query": "isDisabled:n",
            "selection": "single",
            "autocomplete": "0",
            "var": "prompt_answer"
          }
        ]
      }
    },
    {
      "uid": "node_prompt_file",
      "behavior_id": "{{{behavior_id}}}",
      "parent_id": "{{{parent_id}}}",
      "_exclude": "{{% if prompt_input_type != 'file' %}}true{{% endif %}}",
      "type": "action",
      "title": "Prompt for file upload",
      "status": "live",
      "params": {
        "actions": [
          {
            "action": "send_message",
            "message": "Please upload an image file smaller than 250KB.",
            "format": "",
            "delay_ms": "1000"
          },
          {
            "action": "prompt_file",
            "var": "prompt_file_id",
            "var_format": "{{message|round}}",
            "var_validate": "{% if prompt_file_size > 250000 %}\r\nThe file must be smaller than 250KB.\r\n{% elseif not (prompt_file_mime_type starts with 'image/') %}\r\nPlease upload an image: PNG, JPEG, or GIF.\r\n{% endif %}"
          }
        ]
      }
    },
    {
      "uid": "node_prompt_wait",
      "behavior_id": "{{{behavior_id}}}",
      "parent_id": "{{{parent_id}}}",
      "_exclude": "{{% if prompt_input_type != 'wait' %}}true{{% endif %}}",
      "type": "action",
      "title": "Wait",
      "status": "live",
      "params": {
        "actions": [
          {
            "action": "send_message",
            "message": "One moment...",
            "format": "",
            "delay_ms": "1000"
          },
          {
            "action": "prompt_wait"
          }
        ]
      }
    },
    {
      "uid": "node_prompt_images",
      "behavior_id": "{{{behavior_id}}}",
      "parent_id": "{{{parent_id}}}",
      "_exclude": "{{% if prompt_input_type != 'images' %}}true{{% endif %}}",
      "type": "action",
      "title": "Prompt with images",
      "status": "live",
      "params": {
        "actions": [
          {
            "action": "send_message",
            "message": "How are you feeling?",
            "format": "",
            "delay_ms": "1000"
          },
          {
            "action": "prompt_images",
            "images": [
              "data:image/png;base64,",
              "data:image/png;base64,",
              "data:image/png;base64,"
            ],
            "labels": [
              "happy",
              "neutral",
              "unhappy"
            ],
            "var": "prompt_mood",
            "var_format": "{{message}}",
            "var_validate": ""
          }
        ]
      }
    }
  ]
}