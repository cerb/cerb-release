{
  "package": {
    "name": "Page Wizard: Empty Workspace",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Empty Workspace",
      "uri": "cerb_workspace_page_empty",
      "description": "A blank canvas constrained only by your imagination",
      "point": "workspace_page",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "prompts": [
        {
          "type": "text",
          "label": "Owner Context:",
          "key": "owner_context",
          "hidden": true,
          "params": {
          }
        },
        {
          "type": "text",
          "label": "Owner Context ID:",
          "key": "owner_context_id",
          "hidden": true,
          "params": {
          }
        },
        {
          "type": "text",
          "label": "Workspace Name:",
          "key": "prompt_workspace_name",
          "params": {
            "default": "Workspace",
            "placeholder": "(the name of your workspace)"
          }
        }
      ],
      "placeholders": [

      ]
    }
  },
  "records": [
    {
      "uid": "workspace_empty",
      "_context": "workspace_page",
      "name": "{{{prompt_workspace_name}}}",
      "owner__context": "{{{owner_context}}}",
      "owner_id": "{{{owner_context_id}}}",
      "extension_id": "core.workspace.page.workspace"
    }
  ]
}