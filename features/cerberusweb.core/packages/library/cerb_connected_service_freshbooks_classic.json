{
  "package": {
    "name": "Connected Service: Freshbooks Classic",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Freshbooks Classic",
      "uri": "cerb_connected_service_freshbooks_classic",
      "description": "Integration with Freshbooks Classic",
      "instructions": "# Instructions\r\n\r\n* See [**Cerb + Freshbooks Classic**](https://cerb.ai/guides/integrations/freshbooks-classic/)\r\n* Enter the subdomain and client secret below",
      "point": "connected_service",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [],
      "prompts": [
        {
          "type": "text",
          "label": "Subdomain",
          "key": "prompt_subdomain",
          "params": {
            "default": "",
            "placeholder": "(e.g. SUBDOMAIN.freshbooks.com)"
          }
        },
        {
          "type": "text",
          "label": "Client Secret",
          "key": "prompt_client_secret",
          "params": {
            "default": "",
            "placeholder": "(paste your Client Secret)"
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "service_freshbooks_classic",
      "_context": "connected_service",
      "name": "Freshbooks Classic",
      "extension_id": "cerb.service.provider.oauth1",
      "params": {
        "grant_type": "authorization_code",
        "client_id": "{{{prompt_subdomain}}}",
        "client_secret": "{{{prompt_client_secret}}}",
        "request_token_url": "https://{{{prompt_subdomain}}}.freshbooks.com/oauth/oauth_request.php",
        "authentication_url": "https://{{{prompt_subdomain}}}.freshbooks.com/oauth/oauth_authorize.php",
        "access_token_url": "https://{{{prompt_subdomain}}}.freshbooks.com/oauth/oauth_access.php",
        "signature_method": "PLAINTEXT"
      }
    }
  ]
}